public class Instructor {
    private String sueldo;
    public int versueldo;

    public String getSueldo() {
        return sueldo;
    }

    public void setSueldo(String sueldo) {
        this.sueldo = sueldo;
    }

    public int getVersueldo() {
        return versueldo;
    }

    public void setVersueldo(int versueldo) {
        this.versueldo = versueldo;
    }

    public Instructor(int versueldo, String sueldo) {
        this.sueldo = sueldo;
        this.versueldo = versueldo;


    }
}

