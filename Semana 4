public class Client {

    private int edad;
    public String eliminar;

    public int getEdad() {
        return edad;
    }

    public void setEdad(int edad) {
        this.edad = edad;
    }

    public String getEliminar() {
        return eliminar;
    }

    public void setEliminar(String eliminar) {
        this.eliminar = eliminar;
    }

    public Client (int edad, String eliminar) {
        this.edad = edad;
        this.eliminar = eliminar;

    }

    public boolean setEdad() {
        return false;
    }
}

