public class casillero {

    private int id;
    private String tamaño;

    public int getId() {
        return id;
    }

    public void setTamaño(String tamaño) {
        this.tamaño = tamaño;
    }

    public String getTamaño() {
        return tamaño;
    }

    public void setId(int id) {
        this.id = id;
    }

    public casillero(int id, String tamaño) {
        this.id = id;
        this.tamaño = tamaño;

    }
}
